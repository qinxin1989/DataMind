{
  "name": "crawler-management",
  "displayName": "爬虫管理",
  "version": "1.0.1",
  "description": "管理网页抓取模板、定时任务及历史采集数据",
  "author": "System",
  "license": "MIT",
  "type": "business",
  "category": "data",
  "tags": [
    "crawler",
    "data",
    "extraction",
    "template",
    "task"
  ],
  "dependencies": {},
  "backend": {
    "entry": "./backend/index.ts",
    "routes": {
      "prefix": "/api/skills/crawler",
      "file": "./backend/routes.ts"
    }
  },
  "menus": [
    {
      "id": "crawler-management-main",
      "title": "爬虫管理",
      "path": "/ai/crawler",
      "icon": "DatabaseOutlined",
      "parentId": "data-collection",
      "sortOrder": 3,
      "permission": "crawler:view"
    },
    {
      "id": "crawler-results-all",
      "title": "采集结果库",
      "path": "/ai/crawler-results",
      "icon": "TableOutlined",
      "parentId": "data-collection",
      "sortOrder": 4,
      "permission": "crawler:view"
    }
  ],
  "permissions": [
    {
      "code": "crawler:view",
      "name": "查看爬虫",
      "description": "查看爬虫模板、任务和结果"
    },
    {
      "code": "crawler:manage",
      "name": "管理爬虫",
      "description": "创建、编辑、删除爬虫模板和任务"
    },
    {
      "code": "crawler:execute",
      "name": "执行爬虫",
      "description": "手动执行爬虫任务"
    }
  ],
  "api": {
    "endpoints": [
      {
        "method": "GET",
        "path": "/crawler-management/templates",
        "description": "获取爬虫模板",
        "permission": "crawler:view"
      },
      {
        "method": "POST",
        "path": "/crawler-management/templates",
        "description": "创建爬虫模板",
        "permission": "crawler:manage"
      },
      {
        "method": "GET",
        "path": "/crawler-management/tasks",
        "description": "获取爬虫任务",
        "permission": "crawler:view"
      }
    ]
  },
  "enabled": true,
  "installed": true
}