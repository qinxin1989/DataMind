{
  "name": "ai-stats",
  "displayName": "AI统计模块",
  "version": "1.0.0",
  "description": "AI使用统计、对话历史管理和用户统计功能",
  "author": "System",
  "license": "MIT",
  "type": "business",
  "category": "ai",
  "tags": [
    "ai",
    "statistics",
    "conversation",
    "analytics"
  ],
  "dependencies": {},
  "backend": {
    "entry": "./backend/index.ts",
    "routes": {
      "prefix": "/ai",
      "file": "./backend/routes.ts"
    }
  },
  "menus": [
    {
      "id": "ai-stats-menu",
      "title": "AI统计",
      "path": "/ai/stats",
      "icon": "BarChartOutlined",
      "parentId": "ai-center",
      "sortOrder": 1,
      "permission": "ai:view"
    }
  ],
  "permissions": [
    {
      "code": "ai:view",
      "name": "查看AI统计",
      "description": "查看AI使用统计和对话历史"
    },
    {
      "code": "ai:config",
      "name": "配置AI统计",
      "description": "配置AI统计相关设置"
    }
  ],
  "api": {
    "endpoints": [
      {
        "method": "GET",
        "path": "/ai/stats",
        "description": "获取AI使用统计",
        "permission": "ai:view"
      },
      {
        "method": "GET",
        "path": "/ai/conversations",
        "description": "查询对话历史",
        "permission": "ai:view"
      }
    ]
  },
  "enabled": true,
  "installed": true
}